<h1>Login</h1>
<%= form_for(@user_session) do |f| %>
  <%= f.error_messages %>

  <%= f.label :username %><br />
  <%= f.text_field :username %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>